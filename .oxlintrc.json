{
  "env": {
    "browser": true
  },
  "rules": {
    "eqeqeq": "warn",
    "no-eval": "off",
    "import/default": "off", // yet buggy for `?worker` url
  },
  "overrides": [
    {
      "files": ["*.test.ts", "*.spec.ts"],
      "rules": {
        "@typescript-eslint/no-explicit-any": "off"
      }
    }
  ],
  "plugins": ["import", "typescript", "unicorn"]
}
